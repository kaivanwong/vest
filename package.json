{
  "name": "starter-vite-vue3",
  "type": "module",
  "version": "1.3.0",
  "packageManager": "pnpm@8.15.8",
  "description": "Vite + Vue 3 SPA starter template, supports I18n, UnoCSS, Axios and API auto-import.",
  "author": "Kieran Wong <kieranwong9865@gmail.com> (https://github.com/kieranwong9865/)",
  "license": "MIT",
  "engines": {
    "node": ">=18 || >=20"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "simple-git-hooks",
    "dev": "vite dev",
    "dev:test": "vite dev --mode testing",
    "dev:stag": "vite dev --mode staging",
    "dev:prod": "vite dev --mode production",
    "build:dev": "vite build --mode development",
    "build:test": "vite build --mode testing",
    "build:stag": "vite build --mode staging",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "vue-tsc --noEmit",
    "clean": "rimraf dist",
    "release": "bumpp"
  },
  "dependencies": {
    "@unocss/reset": "^0.58.9",
    "axios": "^1.7.3",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.1",
    "vue": "^3.4.37",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.25.0",
    "@iconify-json/carbon": "^1.1.37",
    "@intlify/unplugin-vue-i18n": "^1.6.0",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.19.44",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^10.11.1",
    "bumpp": "^9.4.2",
    "eslint": "^8.57.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.2.8",
    "lodash-es": "^4.17.21",
    "npm-run-all2": "^6.2.2",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.4",
    "unocss": "^0.61.9",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.3",
    "unplugin-vue-router": "^0.10.2",
    "vite": "^5.4.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-vue-devtools": "^7.3.7",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.29"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "pnpm lint:fix"
  }
}
