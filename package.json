{
  "name": "opuntia",
  "type": "module",
  "version": "0.9.3",
  "packageManager": "pnpm@8.13.1",
  "engines": {
    "node": ">=18.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "simple-git-hooks",
    "dev": "vite dev",
    "build-only": "vite build",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test": "run-p test-e2e test-unit",
